{
  "logs": {
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "/var/log/app.log",
            "log_group_name": "/ec2/springboot_logs",
            "log_stream_name": "{instance_id}/app.log",
            "timestamp_format": "%Y-%m-%d %H:%M:%S",
            "multi_line_start_pattern": "^\d{4}-\d{2}-\d{2}"
          }
        ]
      }
    }
  },
  "metrics": {
    "metrics_collected": {
      "application_signals": {}
    }
  },
  "traces": {
    "traces_collected": {
      "application_signals": {}
    }
  }
}
